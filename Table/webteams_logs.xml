<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : webteams.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the webteams_logs table.
-->
<table>
    <column>
        <name>description</name>
        <type>text</type>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>idcontacto</name>
        <type>integer</type>
    </column>
    <column>
        <name>idteam</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>link</name>
        <type>character varying(300)</type>
    </column>
    <column>
        <name>time</name>
        <type>timestamp</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>webteams_logs_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_webteams_logs_contactos</name>
        <type>FOREIGN KEY (idcontacto) REFERENCES contactos (idcontacto) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>