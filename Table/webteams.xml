<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : webteams.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the webteams table.
-->
<table>
    <column>
        <name>creationdate</name>
        <type>date</type>
    </column>
    <column>
        <name>defaultpublication</name>
        <type>integer</type>
    </column>
    <column>
        <name>description</name>
        <type>text</type>
    </column>
    <column>
        <name>idcontacto</name>
        <type>integer</type>
    </column>
    <column>
        <name>idteam</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('webteams_idteam_seq'::regclass)</default>
    </column>
    <column>
        <name>lastip</name>
        <type>character varying(40)</type>
    </column>
    <column>
        <name>lastmod</name>
        <type>date</type>
    </column>
    <column>
        <name>maxinactivitydays</name>
        <type>integer</type>
    </column>
    <column>
        <name>name</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>nummembers</name>
        <type>integer</type>
    </column>
    <column>
        <name>numrequests</name>
        <type>integer</type>
    </column>
    <column>
        <name>private</name>
        <type>boolean</type>
        <default>false</default>
    </column>
    <column>
        <name>visitcount</name>
        <type>integer</type>
    </column>
    <constraint>
        <name>webteams_pkey</name>
        <type>PRIMARY KEY (idteam)</type>
    </constraint>
    <constraint>
        <name>ca_webteams_contactos2</name>
        <type>FOREIGN KEY (idcontacto) REFERENCES contactos (idcontacto) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>uniq_webteams</name>
        <type>UNIQUE (name)</type>
    </constraint>
</table>